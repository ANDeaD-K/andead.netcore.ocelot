{
    "ReRoutes": [{
        "DownstreamHostAndPorts": [
            {
                "Host": "web",
                "Port": 443
            }
        ],
        "DownstreamPathTemplate": "/{url}",
        "UpstreamPathTemplate": "/{url}",
        "UpstreamHttpMethod": [],
        "ReRouteIsCaseSensitive": false,
        "DownstreamScheme": "https",
        "DangerousAcceptAnyServerCertificateValidator": true
    },{
        "DownstreamHostAndPorts": [
            {
                "Host": "oauth",
                "Port": 5051
            }
        ],
        "DownstreamPathTemplate": "/api/oauth/{everything}",
        "UpstreamPathTemplate": "/social/provider/{everything}",
        "UpstreamHttpMethod": [],
        "ReRouteIsCaseSensitive": false,
        "DownstreamScheme": "http"
    },{
        "DownstreamHostAndPorts": [
            {
                "Host": "notifications",
                "Port": 5051
            }
        ],
        "DownstreamPathTemplate": "/api/notifications/{everything}",
        "UpstreamPathTemplate": "/notifications/{everything}",
        "UpstreamHttpMethod": [],
        "ReRouteIsCaseSensitive": false,
        "DownstreamScheme": "http",
        "AuthenticationOptions": {
            "AuthenticationProviderKey": "AuthKey",
            "AllowedScopes": []
        },
        "AddQueriesToRequest": {
            "userId": "Claims[http://schemas.xmlsoap.org/ws/2005/05/identity/claims/nameidentifier] > value"
        }
    },{
        "DownstreamHostAndPorts": [
            {
                "Host": "mqtt",
                "Port": 5051
            }
        ],
        "DownstreamPathTemplate": "/api/devices/{everything}",
        "UpstreamPathTemplate": "/devices/{everything}",
        "UpstreamHttpMethod": [],
        "ReRouteIsCaseSensitive": false,
        "DownstreamScheme": "http",
        "AuthenticationOptions": {
            "AuthenticationProviderKey": "AuthKey",
            "AllowedScopes": []
        },
        "AddQueriesToRequest": {
            "userId": "Claims[http://schemas.xmlsoap.org/ws/2005/05/identity/claims/nameidentifier] > value"
        }
    }]
}
